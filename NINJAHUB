local Players = game:GetService("Players")
local function executeAfterGui()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end

local marco = game.Workspace.Characters.NPCs.Marco

if pcall(function() return game.Workspace.Characters.NPCs.Marco end) then
    local gui = Instance.new("ScreenGui")
    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0.5, 0, 0.2, 0)
    frame.Position = UDim2.new(0.25, 0, 0.4, 0)
    frame.BackgroundColor3 = Color3.new(1, 1, 1)
    frame.BorderSizePixel = 0
    local textLabel = Instance.new("TextLabel", frame)
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.Text = "His here"
    textLabel.FontSize = Enum.FontSize.Size24
    textLabel.TextColor3 = Color3.new(0, 0, 0)
    textLabel.BackgroundTransparency = 1
    gui.Parent = game:GetService("StarterGui")

    wait(2)

    gui:Destroy()
    
    wait(1)
    Players.LocalPlayer.Character.HumanoidRootPart.CFrame = marco.HumanoidRootPart.CFrame
    executeAfterGui()
else
    local gui = Instance.new("ScreenGui")
    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0.5, 0, 0.2, 0)
    frame.Position = UDim2.new(0.25, 0, 0.4, 0)
    frame.BackgroundColor3 = Color3.new(1, 1, 1)
    frame.BorderSizePixel = 0
    local textLabel = Instance.new("TextLabel", frame)
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.Text = "His not here"
    textLabel.FontSize = Enum.FontSize.Size24
    textLabel.TextColor3 = Color3.new(0, 0, 0)
    textLabel.BackgroundTransparency = 1
    gui.Parent = game:GetService("StarterGui")

    wait(2)

    gui:Destroy()
    executeAfterGui()
end
